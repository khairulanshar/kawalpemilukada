<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>org.kawalpemilukada.login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>callbackfb</servlet-name>
        <servlet-class>org.kawalpemilukada.login.callbackfb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>org.kawalpemilukada.login.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getModelData</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.getModelData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>callbackTwit</servlet-name>
        <servlet-class>org.kawalpemilukada.login.callbackTwit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>wilayah</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.wilayah</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>kandidat</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.kandidat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>serve</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.serve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pesan</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.pesan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>suara</servlet-name>
        <servlet-class>org.kawalpemilukada.web.controller.suara</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>callbackfb</servlet-name>
        <url-pattern>/callbackfb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getModelData</servlet-name>
        <url-pattern>/getModelData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>callbackTwit</servlet-name>
        <url-pattern>/callbackTwit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>wilayah</servlet-name>
        <url-pattern>/wilayah/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>kandidat</servlet-name>
        <url-pattern>/kandidat/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>serve</servlet-name>
        <url-pattern>/serve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pesan</servlet-name>
        <url-pattern>/pesan/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>suara</servlet-name>
        <url-pattern>/suara/*</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>pages/index.html</welcome-file>
    </welcome-file-list>
    <!-- [START Objectify] -->
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>org.kawalpemilukada.model.OfyHelper</listener-class>
    </listener>
    <!-- [END Objectify] -->
